{
  "name": "benahmetcelik/laravel-menu",
  "description": "drag and drop menu generator for laravel 5... Laravel 12",
  "keywords": [
    "Laravel",
    "Menu",
    "Bootstrap",
    "Builder"
  ],
  "authors": [
    {
      "name": "Ahmet CELIK",
      "email": "ben4hmetcelik@gmail.com",
      "role": "Developer"
    },
    {
      "name": "Nguyen Dac Huy",
      "email": "huy.cit@gmail.com",
      "role": "Original Developer"
    }
  ],
  "require": {
    "php": ">=7.0",
    "illuminate/support": ">=v5.0.0"
  },
  "autoload": {
    "psr-4": {
      "NguyenHuy\\Menu\\": "src"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "NguyenHuy\\Menu\\Providers\\MenuServiceProvider"
      ],
      "aliases": {
        "Menu": "NguyenHuy\\Menu\\Facades\\Menu"
      }
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}